{
	"compilerOptions": {
		"target": "ES6",
		"module": "ES6",
		"strict": true,
		"jsx": "react-jsx",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"types": ["webpack-env"]
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
